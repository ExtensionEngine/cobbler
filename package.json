    "dotenv": "^8.2.0",
    "eslint-plugin-require-sort": "^1.2.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "query-string": "^6.13.1",
    "sequelize": "^5.21.12",
    "thwack": "^0.6.0",
    "superstruct": "^0.10.12",
    "umzug": "^2.3.0",
    "vee-validate": "^3.3.4",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@extensionengine/eslint-config": "^1.2.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.1",
