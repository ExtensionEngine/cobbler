    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/cobbler.git"
